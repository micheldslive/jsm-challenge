    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react-merge-refs": "^2.1.1",
    "tailwind-variants": "^0.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  }
